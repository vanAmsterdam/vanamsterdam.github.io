  author = {Eliceiri, Kevin W and Berthold, Michael R and Goldberg, Ilya G and Ibáñez, Luis and family=Manjunath, given=BS, given-i=BS and Martone, Maryann E and Murphy, Robert F and Peng, Hanchuan and Plant, Anne L and Roysam, Badrinath and Stuurman, Nico and Stuurmann, Nico and Swedlow, Jason R and Tomancak, Pavel and Carpenter, Anne E},
