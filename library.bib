}

@article{cowenMicroscopicalAssayUsing1992,
  title = {A Microscopical Assay Using a Densitometric Application of Image Analysis to Quantify Neurotransmitter Dynamics},
  author = {Cowen, T and Thrasivoulou, C},
  year = {1992 Oct-Nov},
  journaltitle = {Journal of neuroscience methods},
