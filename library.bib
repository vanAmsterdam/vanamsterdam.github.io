@article{inouePretreatmentTumorSize2020,
  title = {Pre-Treatment Tumor Size Impacts on Response to Nivolumab in Head and Neck Squamous Cell Carcinoma},
  author = {Inoue, Hiroto and Yokota, Tomoya and Hamauchi, Satoshi and Onozawa, Yusuke and Kawakami, Takeshi and Shirasu, Hiromichi and Notsu, Akifumi and Yasui, Hirofumi and Onitsuka, Tetsuro},
  date = {2020-08-01},
  journaltitle = {Auris Nasus Larynx},
