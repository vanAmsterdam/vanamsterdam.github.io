  keywords = {clinical assessment,research,statistics}
}

@article{gossiosRenalDenervationTransition2014,
  title = {Renal {{Denervation Transition From Pathophysiology}} to {{Clinical Practice}}},
  author = {Gossios, Thomas D. and Ziakas, Antonios and Doumas, Michalis and Katsiki, Niki and Petroglou, Dimitrios and Kouparanis, Antonios and Lillis, Leonidas and Karvounis, Haralambos},
