  author = {Dercle, L. and Ammari, S. and Champiat, S. and Massard, C. and Ferté, C. and Taihi, L. and Seban, R.-D. and Aspeslagh, S. and Mahjoubi, L. and Kamsu-Kom, N. and Robert, C. and Marabelle, A. and Schlumberger, M. and Soria, J.-C. and Postel-Vinay, S.},
