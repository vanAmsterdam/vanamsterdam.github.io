  title = {Impact of {{Sarcopenia}} on {{Survival}} in {{Patients With Early-Stage Lung Cancer Treated With Stereotactic Body Radiation Therapy}}.},
  author = {Taylor, James M. and Song, Andrew and David, Allison R. and Chen, Victor E. and Lu, Bo and Werner-Wasik, Maria},
